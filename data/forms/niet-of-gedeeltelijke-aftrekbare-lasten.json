            "postKey": "119356-EB",
            "customValue": {
              "formula": "minusSumOfFields",
              "parameters": {
                "fieldModels": [
                  "ondernemingsvermogenExclFiscaleEeserves",
                  "representatiekostenMutatie",
                  "reisEnVerblijfkostenMutatie",
                  "nietAftrekbareKostenLastenMutatie",
